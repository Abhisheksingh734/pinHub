<% include ./partials/header.ejs %>
<div class="w-full min-h-screen px-10 py-8 bg-zinc-800 text-white">
    <div class="cards flex flex-wrap gap-10">
        <% posts.forEach((ele)=>{ %>
<div class="card w-32">

<div class=" w-full h-40 rounded-xl overflow-hidden">
    <img src="/images/uploads/<%-ele.image%>"  class="w-full h-full object-cover" alt="">
    
</div>
<h5 class="mt-2 text-sm"><%=ele.title %></h5>
<h6 class="text-xs text-zinc-400">@<%=ele.user.username %></h6>
</div>
       <% }) %>
        
    </div>
</div>
<% include ./partials/footer.ejs %>
